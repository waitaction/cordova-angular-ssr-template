    "test:e2e": "node ./hooks/compile-hook/test-e2e.js",
    "test:component": "node ./hooks/compile-hook/test-component.js",
    "stats": "ng build --prod --stats-json && webpack-bundle-analyzer www/stats-es2015.json",
