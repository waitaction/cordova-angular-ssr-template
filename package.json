{
  "name": "angular-ssr-template",
  "version": "1.0.0",
  "scripts": {
    "start:browser:dev": "node ./hooks/compile-hook/debug-browser.js",
    "build:browser": "node ./hooks/compile-hook/build-browser.js",
    "start:android:dev": "node ./hooks/compile-hook/debug-android.js",
    "build:android": "node ./hooks/compile-hook/build-android.js",
    "start:ios:dev": "node ./hooks/compile-hook/debug-ios.js",
    "build:ios": "node ./hooks/compile-hook/build-ios.js",
